module multiplexor#(parameter N=4)(input [N-1:0] i0, i1, i2, i3, i4, i5, i6, input[3:0] s, output [N-1:0] out);
	always @(s) begin 
		case (s)
			0:
				out = i0;
			1:
				out = i1;
			2:
				out = i2;
			3:
				out = i3;
			4:
				out = i4;
			5:
				out = i5;
			6:
				out = i6;
			default: 
				out = 0;
		endcase
	end
endmodule